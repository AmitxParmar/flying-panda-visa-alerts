generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}


// User Model
model User {
    id             String   @id @default(auto()) @map("_id") @db.ObjectId
    name           String?
    password       String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@map("users")
}

